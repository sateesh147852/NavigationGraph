<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.navigationgraph.ui.HomeFragment"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_FAQFragment"
            app:destination="@id/FAQFragment" />
    </fragment>

    <fragment
        android:id="@+id/accountFragment"
        android:name="com.navigationgraph.ui.AccountFragment"
        tools:layout="@layout/fragment_account" />

    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.navigationgraph.ui.SettingsFragment"
        tools:layout="@layout/fragment_settings" />
    <fragment
        android:id="@+id/FAQFragment"
        android:name="com.navigationgraph.ui.FAQFragment"
        tools:layout="@layout/fragment_f_a_q" />

</navigation>